@model IEnumerable<IMS.DataAccess.OnSiteOperator>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            Name
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OperatorID }) |
            @Html.ActionLink("Details", "Details", new { id = item.OperatorID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.OperatorID })
        </td>
        <td>
            @item.Name
        </td>
    </tr>
}

</table>

